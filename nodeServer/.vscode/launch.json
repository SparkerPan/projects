            "runtimeExecutable": "D:/App_work/Browser_S/Chrome/Google/Chrome/Application/chrome.exe",
            "runtimeArgs": ["--auction_server.js"],
            "userDataDir": "${workspaceRoot}/.vscode/chrome",
            "webRoot": "${workspaceFolder}"
        }
    ]
