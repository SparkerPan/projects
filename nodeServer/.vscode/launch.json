    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node服务端",
            "program": "${workspaceFolder}/build/auction_server.js"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome调试",
            "url": "http://localhost:8000",
